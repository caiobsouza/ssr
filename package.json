{
  "name": "mosaico-spa",
  "version": "1.0.1",
  "description": "",
  "main": "webpack.config.js",
  "scripts": {
    "build": "NODE_ENV=production webpack --progress",
    "build:dev": "webpack --watch --progress",
    "start": "NODE_ENV=production node index.js",
    "start:dev": "nodemon index.js",
    "dev": "webpack --watch --progress & nodemon index.js",
    "linter": "eslint src",
    "bundlesize": "bundlesize",
    "precommit": "eslint src --quiet",
    "test": "jest",
    "lighthouse": "lighthouse http://localhost:3030 --view --output html --output-path ./lighthouse/report.html"
  },
  "dependencies": {
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-helmet": "^5.2.0",
    "react-router-dom": "^4.2.2",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@babel/core": "^7.4.3",
    "@babel/plugin-proposal-class-properties": "^7.4.0",
    "@babel/preset-env": "^7.4.2",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.4.0",
    "babel-eslint": "^8.0.2",
    "babel-jest": "^24.7.1",
    "babel-loader": "^8.0.5",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "bundlesize": "^0.17.1",
    "enzyme": "^3.2.0",
    "enzyme-adapter-react-16": "^1.1.0",
    "eslint": "^4.12.0",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.5.1",
    "express": "^4.16.3",
    "friendly-errors-webpack-plugin": "^1.6.1",
    "husky": "^0.14.3",
    "jest": "^24.7.1",
    "lighthouse": "^4.3.0",
    "nodemon": "^1.17.2",
    "webpack": "^4.0.0",
    "webpack-bundle-analyzer": "^2.9.1",
    "webpack-cli": "^3.1.1",
    "webpack-dev-server": "^3.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "Firefox ESR"
  ],
  "bundlesize": [
    {
      "path": "./dist/js/app.bundle.js",
      "maxSize": "170 kB"
    },
    {
      "path": "./dist/js/lib.bundle.js",
      "maxSize": "110 kB"
    }
  ],
  "jest": {
    "transform": {
      "^.+\\.jsx$": "babel-jest",
      "^.+\\.js$": "babel-jest"
    },
    "setupTestFrameworkScriptFile": "<rootDir>/test-config/test-setup.js"
  },
  "author": "",
  "license": "ISC"
}
